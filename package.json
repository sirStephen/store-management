{
  "name": "store-manager",
  "version": "1.0.0",
  "description": "Store Manager is a web application that helps store owners manage sales and product inventory  records. This application is meant for use in a single store.",
  "main": "server.js",
  "scripts": {
    "test": "mocha 'server/test/*.js' --timeout 10000 --require babel-core/register --exit",
    "dev-start": "nodemon --exec babel-watch server/server.js",
    "cover": "istanbul cover _mocha",
    "coveralls": "npm run cover -- --report lcovonly && cat ./coverage/lcov.info | coveralls",
    "start": "babel-node server/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sirStephen/store-management.git"
  },
  "author": "Gregory Otiono",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sirStephen/store-management/issues"
  },
  "dependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.2.0",
    "coveralls": "^3.0.2",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/register": "^7.0.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-watch": "^2.0.7",
    "coffeescript": "^2.3.2",
    "eslint": "^5.7.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "nodemon": "^1.18.4"
  },
  "engines": {
    "node": "9.11.2"
  }
}